{
  "targets": {
    "tcm": {
      "executor": "nx:run-commands",
      "options": {
        "command": "tcm src/",
        "cwd": "libs/ui-generic"
      }
    },
    "tsc": {
      "executor": "@nx/js:tsc",
      "options": {
        "outputPath": "libs/ui-generic/dist",
        "main": "libs/ui-generic/src/index.ts",
        "tsConfig": "libs/ui-generic/tsconfig.lib.json",
        "assets": [
          "libs/ui-generic/src/**/*.module.css"
        ]
      },
      "dependsOn": [
        "tcm"
      ]
    },
    "build": {
      "dependsOn": [
        "tsc"
      ]
    }
  }
}